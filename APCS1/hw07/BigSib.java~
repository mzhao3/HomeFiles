public class BigSib {
    private String helloMsg;
    public BigSib() { 
	helloMsg = "Welcome" ;
    }
    public BigSib(String newMsg){
	helloMsg = newMsg;
    }
    
    public String greet (String name) {
	return (helloMsg + " " + name);
	String result;
	result = helloMsg;
	result += " ";
	result += name;
	return result; 

    }
}
